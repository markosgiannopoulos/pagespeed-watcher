{
  "name": "apogee/laravel-pagespeed-watcher",
  "description": "PageSpeed Watcher: A Laravel package to run Google PageSpeed Insights (PSI) checks for your app's pages.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "pagespeed",
    "psi",
    "performance",
    "watcher"
  ],
  "authors": [
    {
      "name": "Apogee",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0|^12.0|^13.0",
    "illuminate/console": "^11.0|^12.0|^13.0",
    "guzzlehttp/guzzle": "^7.9",
    "symfony/dom-crawler": "^7.0",
    "symfony/css-selector": "^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^10.5",
    "mockery/mockery": "^1.6",
    "orchestra/testbench-core": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Apogee\\Watcher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apogee\\Watcher\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Apogee\\Watcher\\WatcherServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "lint": "vendor/bin/phpcs --standard=PSR12 src --warning-severity=0 || true"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}